{
  "name": "ai-mad-libs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk:synth": "npx cdk synth",
    "cdk:deploy": "npx cdk deploy",
    "cdk:destroy": "npx cdk destroy",
    "cdk:diff": "npx cdk diff",
    "cdk:bootstrap": "npx cdk bootstrap",
    "cdk:watch": "npx cdk watch",
    "cdk:ls": "npx cdk ls",
    "deploy:dev": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Environment development",
    "deploy:staging": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Environment staging",
    "deploy:prod": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Environment production",
    "deploy:serverless": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "setup:deploy-user": "powershell -ExecutionPolicy Bypass -File scripts/create-deploy-user.ps1",
    "setup:deploy-user-cf": "aws cloudformation create-stack --stack-name mad-libs-deploy-user --template-body file://cloudformation/deploy-user-stack.yaml --capabilities CAPABILITY_NAMED_IAM",
    "test:cdk": "jest cdk/__tests__",
    "build:lambda": "cd lambda && npm install && npm run build",
    "clean:lambda": "cd lambda && npm run clean",
    "check:deployment": "powershell -ExecutionPolicy Bypass -File scripts/check-deployment-readiness.ps1",
    "verify:deploy-user": "powershell -ExecutionPolicy Bypass -File scripts/verify-deploy-user.ps1",
    "config:deployment": "powershell -ExecutionPolicy Bypass -File scripts/configure-deployment.ps1",
    "setup:environment": "powershell -ExecutionPolicy Bypass -File scripts/setup-environment.ps1"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.450.0",
    "@aws-sdk/client-bedrock-runtime": "^3.864.0",
    "@aws-sdk/client-mediaconvert": "^3.864.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "aws-cdk-lib": "^2.211.0",
    "constructs": "^10.4.2",
    "dotenv": "^17.2.1",
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-cdk/assertions": "^1.203.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^20.19.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.0.1",
    "aws-cdk": "^2.1025.0",
    "esbuild": "^0.19.12",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
